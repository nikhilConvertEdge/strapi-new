        "section.e-book-beginner-guide",
        "section.about-e-book",
        "section.recommended-articles",
        "section.recommended-course"
      ]
    },
    "b_2_b_resource_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::b2-b-resource-type.b2-b-resource-type"
    },
    "b_2_b_author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::b2-b-author.b2-b-author"
    }
